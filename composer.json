{
	"name": "fromholdio/silverstripe-sortable",
	"type": "silverstripe-vendormodule",
	"description": "Adds an extension to make any SilverStripe DataObject sortable.",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "dataobject", "sort", "orderable", "sortable"],
	"homepage": "https://github.com/fromholdio/silverstripe-sortable",
	"support": {
		"issues": "https://github.com/fromholdio/silverstripe-sortable/issues"
	},
	"authors": [{
		"name": "Luke Fromhold",
		"homepage": "https://fromhold.io"
	}],
	"require": {
        "silverstripe/framework": "^6.0"
	},
	"autoload": {
		"psr-4": {
			"Fromholdio\\Sortable\\": "src/"
		}
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
